.cal {
    float: left;
    width: 39px;
    height: 39px;
    margin: 3px 10px 0 0;
    background: transparent asset-url('login_area.png', image) no-repeat top left;
    /* For Sprite */
    background-position: 0 0;
    width: 39px;
    height: 39px;
    /* For Sprite */
    text-align: center;
    font-family: Arial;
}

#reports .cal .day {
    line-height: 1;
    font-size: 18px;
    display: block;
    position: relative;
    top: 4px;
}

#reports .cal .mon {
    font-size: 9px;
    line-height: 1;
    text-transform: uppercase;
    display: block;
    color: white;
    position: relative;
    bottom: -8px;
}

#reports ul {
    margin-left: 0px;
    padding: 0px;
}

#reports li {
    list-style-type: none;
    margin-bottom: 5px;
}

.clear {
  clear: both;
}

.seperator{
  margin: 5px;
  border-bottom: 1px dotted #999
}

#scroll-pane {
  float:left;
  overflow:auto; width:300px;
  height: 200px;
  position: relative;
  margin-left: 50px;
  margin-bottom: 25px;
  display: inline
}

.scroll-content {
  position:absolute;
  top:0;
  left:0
}

#slider-wrap {
  float:left;
  background-color:lightgrey;
  width:20px;
  border:1px solid gray;
  border-left:none;
}
#slider-vertical {
  position:relative;
  height:100%
}
.ui-slider-handle {
  width:20px;
  height:10px;
  margin:0 auto;
  background-color:darkgray;
  display:block;
  position:absolute
}

#reports ul {
    margin-left: 0px;
    padding: 0px;
}

#reports li {
    list-style-type: none;
    margin-bottom: 5px;
}